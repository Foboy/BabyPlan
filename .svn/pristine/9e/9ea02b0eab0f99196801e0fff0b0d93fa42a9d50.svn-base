.bookName{width:960px; margin-top:20px;}
.bookName label{font-size:16px; width:140px; display:inline-block; float:left; margin-top:10px;}
.bookName .input_box{width:250px; float:left;}
.bookStep dl dt{background:#2a8fc5; height:50px; line-height:50px; font-size:20px; color:#FFFFFF; cursor:pointer; margin-top:20px;}
.bookStep dl dt span{margin-left:10px;}
.bookStep dl dt.none{background:#F5F5F5; color:#666666;}
.bookStep dl dt b{background:url(/images/circle.png) no-repeat; display:inline-block; width:35px; height:35px; line-height:35px;text-align:center; margin:0 10px;}
.bookStep dl dd{background:#F5F5F5; width:958px; padding-bottom:20px;}
.pageSize div{background:#FFFFFF; padding:10px; width:200px; margin:10px 50px 0 55px; float:left;}
.pageSize div.cur{background:#2a8fc5;}
.pageSize div p{background:#FFFFFF;border:1px solid #e5e5e5; height:240px; font-size:46px; text-align:center; line-height:240px; color:#e5e5e5;}
.formworkL{width:28px; height:170px;}
.formworkL span,.formworkR span{background:url(/images/arrows.png) no-repeat; width:16px; height:26px; margin-top:80px; display:inline-block; cursor:pointer;}
.formworkL span{background-position:-2px 0; margin-left:12px;}
.formworkR span{background-position:-30px 0;}
.coverL span{margin-top:125px;}
.formworkM ul,.cover ul{margin:20px 0 0 0;}
.cover{margin-left:-3px;}
.formworkM ul li,.cover ul li{float:left; text-align:center; width:110px; height:134px; padding:8px; background:#FFFFFF; margin:0 27px; cursor:pointer;}
.cover ul li{width:150px;height:219px; margin:0 30px;}
.formworkM ul li:hover,.formworkM ul li.cur,.cover ul li:hover,.cover ul li.cur{background:#2a8fc5;}
.contentIntro{border-bottom:1px dashed #DEDEDE; padding:20px 0 20px 34px; overflow:hidden;}
.contentIntro .black-font{margin-right:20px;}
.example{ margin-left:10px; width:260px; height:92px;}
.update{padding:20px 0 5px 0; width:960px;}
.update .updateLocal{width:100px; height:30px; line-height:30px; font-size:14px; margin-left:34px; display:inline-block; }
.imgShow{margin:20px 0 0 32px; border:1px solid #DEDEDE; background:#FFFFFF;}
.imgShow img,.textDes{float:left;}
.imgShow img{ margin-right:10px;}
.textDes{width:256px; height:86px; padding:5px;}
.textDes h2{font-weight:normal; font-size:12px; margin-bottom:5px;}
.textDes p{line-height:16px;}
.textDes .area1,.textDes .area2{width:254px; height:60px; font-size:12px; border:1px solid #DEDEDE;}
.textDes .area1{height:24px; border:none; color:#000000;}
.textDes .area1 input{width:120px; border:1px solid #DEDEDE;}
.makeBook {height: 60px;line-height: 60px;margin: 20px auto 50px;width: 180px;}
.upload_box{width:140px;height:90px;padding:5px;float:left;}